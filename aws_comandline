
#aws linux
aws ec2 run-instances --image-id ami-162c2575 --placement AvailabilityZone=ap-southeast-2a --count 1 --instance-type t2.micro --key-name MyEC2Key --user-data file:///Users/stephenkennedy-clark/Documents/Steve/programming/maquarie_job_app/awsscript.sh --security-groups Web-DMZ  --iam-instance-profile Name=S3-Admin-Access

#ubuntu 
aws ec2 run-instances --image-id ami-96666ff5 --placement AvailabilityZone=ap-southeast-2a --count 1 --instance-type t2.micro --key-name MyEC2Key --user-data file:///Users/stephenkennedy-clark/Documents/Steve/programming/maquarie_job_app/awsscript_ubuntu.sh --security-groups Web-DMZ  --iam-instance-profile Name=S3-Admin-Access


aws ec2 describe-instances | grep PublicIpAddress

aws ec2 describe-instances | grep InstanceId

aws s3 cp create_db.SQL s3://mq-random-forest/create_db.SQL

aws s3 cp app.py s3://mq-random-forest/app.py

aws ec2 terminate-instances --instance-ids i-0726fd578fb7ee165

ssh -i ~/.ssh/MyEC2Key.pem ec2-user@

ssh -i ~/.ssh/MyEC2Key.pem ubuntu@


cd /Users/stephenkennedy-clark/Documents/Steve/programming/maquarie_job_app 
tail  -n100 /var/log/cloud-init-output.log
sudo lsof -i:80
sudo kill